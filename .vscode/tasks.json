{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Run Web App (with Auto-Advance)",
			"type": "shell",
			"command": "dotnet",
			"args": [
				"watch",
				"run",
				"--project",
				"CycleRoutes/CycleRoutes.Web"
			],
			"group": "build",
			"isBackground": true,
			"problemMatcher": [
				"$tsc"
			]
		}
	]
}